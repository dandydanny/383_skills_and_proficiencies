<div class="container">
  <h1>User Skills and Levels</h1>

  <%# TODO: display the skills and proficiency levels for each user ... %>

  <table>
    <tr>
      <td>Name</td>
      <td>Skill</td>
      <td>Context</td>
      <td>Years</td>
      <td>Formal?</td>
    </tr>
    <% @skills.each do |skill| %>
    <tr>
      <td><%= skill.name %></td>
      <td><%= skill.context %></td>
      <td>Context</td>
      <td>Years</td>
      <td>Formal?</td>
    </tr>
    <% end %>
  </table>
</div>
